{
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@3.0.0"
    }
  },
  "rewrites": [
    {
      "source": "/manifest.json",
      "destination": "/api/index.js"
    },
    {
      "source": "/manifest",
      "destination": "/api/index.js"
    },
    {
      "source": "/catalog/:type/:id.json",
      "destination": "/api/index.js"
    },
    {
      "source": "/catalog/:type/:id",
      "destination": "/api/index.js"
    },
    {
      "source": "/:path*",
      "destination": "/api/index.js"
    }
  ]
}
